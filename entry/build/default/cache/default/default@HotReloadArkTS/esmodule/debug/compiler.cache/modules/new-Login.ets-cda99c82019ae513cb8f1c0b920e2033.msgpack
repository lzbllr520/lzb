    //å¯¹ç”¨æˆ·è¾“å…¥çš„å¯†ç è¿›è¡ŒåŠ å¯†
    const encryptedPassword = await this.encryptPassword(this.password);
    // å¦‚æœåŠ å¯†è¿‡ç¨‹å‡ºé”™ï¼Œåˆ™å‡½æ•°ç»ˆæ­¢
    if (!encryptedPassword) {
