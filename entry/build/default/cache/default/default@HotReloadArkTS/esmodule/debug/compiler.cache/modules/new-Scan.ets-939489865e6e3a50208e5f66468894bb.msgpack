          .animation({
            duration: 2000,
            curve: Curve.EaseInOut,
            iterations: -1,
            playMode: PlayMode.Normal,
            onFinish: () => {
              const start = this.scanLineTop;
              this.scanLineTop = start === 0 ? 248 : 0;
            }
          })
      }
    }
    .width(250)
    .height(250)
  }

