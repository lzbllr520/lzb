  // --- ã€æ–°å¢éƒ¨åˆ†ã€‘ ä½¿ç”¨Canvasç»˜åˆ¶å››ä¸ªè§’ ---
  @Builder ScanCorners() {
    Canvas(this.canvasContext)
      .width(250)
      .height(250)
      .onReady(() => {
        // --- å¯è‡ªå®šä¹‰å‚æ•° ---
        const cornerLength: number = 20; // è§’çš„è¾¹é•¿
        const lineWidth: number = 4;      // çº¿çš„å®½åº¦
        const lineColor: string = '#00FF00'; // çº¿çš„é¢œè‰²ï¼ˆç»¿è‰²ï¼‰
        // --------------------

        const size = 250; // ç”»å¸ƒå°ºå¯¸
        const ctx = this.canvasContext;

        // è®¾ç½®çº¿æ®µæ ·å¼
        ctx.lineWidth = lineWidth;
        ctx.strokeStyle = lineColor;

        // æ¸…é™¤æ—§çš„ç»˜åˆ¶ï¼ˆå¦‚æœéœ€è¦é‡ç»˜ï¼‰
        ctx.clearRect(0, 0, size, size);

        // ç»˜åˆ¶å·¦ä¸Šè§’
        ctx.beginPath();
        ctx.moveTo(cornerLength, 0);
        ctx.lineTo(0, 0);
        ctx.lineTo(0, cornerLength);
        ctx.stroke();

        // ç»˜åˆ¶å³ä¸Šè§’
        ctx.beginPath();
        ctx.moveTo(size - cornerLength, 0);
        ctx.lineTo(size, 0);
        ctx.lineTo(size, cornerLength);
        ctx.stroke();

        // ç»˜åˆ¶å·¦ä¸‹è§’
        ctx.beginPath();
        ctx.moveTo(0, size - cornerLength);
        ctx.lineTo(0, size);
        ctx.lineTo(cornerLength, size);
        ctx.stroke();

        // ç»˜åˆ¶å³ä¸‹è§’
        ctx.beginPath();
        ctx.moveTo(size - cornerLength, size);
        ctx.lineTo(size, size);
        ctx.lineTo(size, size - cornerLength);
        ctx.stroke();
      })
  }

  // --- ã€ä¿®æ”¹éƒ¨åˆ†ã€‘ ScanBoxæ„å»ºå™¨ ---
  @Builder ScanBox() {
    Stack() {
      // 1. ä½¿ç”¨Canvasç»˜åˆ¶å››ä¸ªè§’ä½œä¸ºåº•å±‚
      this.ScanCorners()

      // 2. åŠ¨ç”»æ‰«æçº¿å åŠ åœ¨ä¸Šé¢
      if (this.isAnimating) {
        Rect()
          .width('98%')
          .height(3)
          .fill(Color.Green)
          .shadow({ radius: 10, color: Color.Green })
          .offset({ x: 0, y: this.scanLineTop })
          .onAppear(() => {
