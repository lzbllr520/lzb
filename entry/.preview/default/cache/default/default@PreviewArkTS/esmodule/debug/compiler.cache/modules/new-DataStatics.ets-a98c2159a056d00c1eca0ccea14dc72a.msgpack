import { LvMarkdownIn, lvText, lvTitle, lvLink, lvCode, lvQuote } from '@luvi/lv-markdown-in';

interface KnowledgeInfo {
  id: number;
  image: Resource;
  title: string;
  description: string;
  fullDescription: string;
}

@Component
struct KnowledgeCard {
  // 让父组件可以传入数据
  @Prop info: KnowledgeInfo;
  onCardClick: (info: KnowledgeInfo) => void = () => {};

  @State private isPressed: boolean = false;

  build() {
    Column({ space: 12 }) {
      Text(this.info.title)
        .fontSize(20)
        .fontWeight(FontWeight.Bold)
        .fontColor(Color.White)
        .width('100%')
        .textAlign(TextAlign.Start)
        .textOverflow({ overflow: TextOverflow.Ellipsis })
        .maxLines(2)

      Text(this.info.description)
        .fontSize(14)
        .fontColor('rgba(255, 255, 255, 0.75)')
        .width('100%')
        .textAlign(TextAlign.Start)
        .textOverflow({ overflow: TextOverflow.Ellipsis })
        .maxLines(3)
        .lineHeight(20)
    }
    .padding(20)
    .width('100%')
    .height(160)
    .alignItems(HorizontalAlign.Start)
    .justifyContent(FlexAlign.Start)
    .borderRadius(16)
    .backgroundColor('rgba(10, 10, 15, 0.3)')
    .backdropBlur(12)
    .backgroundColor('rgba(10, 10, 15, 0.3)')
    .borderRadius(16)
    //添加清晰的轮廓线
    .border({
      width: 1.5,
      color: 'rgba(255, 255, 255, 0.15)'
    })
    //添加柔和的背景光晕
    .shadow({
      radius: 30,
      color: 'rgba(173, 216, 230, 0.2)',
      offsetX: 0,
      offsetY: 0
    })
    .clip(true)
    .scale({ x: this.isPressed ? 0.97 : 1.0, y: this.isPressed ? 0.97 : 1.0 })
    .animation({ duration: 150, curve: Curve.EaseInOut })
    .onClick(() => {
      this.onCardClick(this.info);
    })
    .onTouch((event: TouchEvent) => {
      if (event.type === TouchType.Down) { this.isPressed = true; }
      if (event.type === TouchType.Up || event.type === TouchType.Cancel) { this.isPressed = false; }
    })
  }
}

@CustomDialog
struct KnowledgeDetailDialog {
  @Link item: KnowledgeInfo | null;
  controller: CustomDialogController;

  build() {
    Column({ space: 15 }) {
      Text(this.item?.title)
        .fontSize(22)
        .fontWeight(FontWeight.Bold)
        .fontColor(Color.White)

      Divider().color('rgba(255, 255, 255, 0.2)')

      Scroll() {
        LvMarkdownIn({ text: this.item?.fullDescription ?? '' })
      }
      .scrollBar(BarState.Auto)
      .layoutWeight(1)

      Button('关闭')
        .width('100%')
        .height(45)
        .type(ButtonType.Capsule)
        .backgroundColor('rgba(255, 255, 255, 0.2)')
        .onClick(() => {
          this.controller.close();
        })
    }
    .padding(24)
    .width('80%')
    .height('70%')
    .borderRadius(24)
    .backgroundColor('rgba(30, 30, 40, 1.00)')
    .backdropBlur(20)
    .border({ width: 1.5, color: 'rgba(255, 255, 255, 0.2)' })
  }
}


